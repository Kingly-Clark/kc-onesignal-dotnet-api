<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <TargetFramework>netstandard2.1</TargetFramework>
    <AssemblyName>OneSignalApi</AssemblyName>
    <PackageId>OneSignalApi</PackageId>
    <OutputType>Library</OutputType>
    <Authors>OneSignal</Authors>
    <Company>OneSignal</Company>
    <AssemblyTitle>OneSignal API Library</AssemblyTitle>
    <Description>OneSignal is a free push notification service for mobile apps. Integrate OneSignal with your backend events, data, and more.</Description>
    <Copyright>Copyrite 2022 OneSignal</Copyright>
    <RootNamespace>OneSignalApi</RootNamespace>
    <Version>5.3.0-beta1</Version>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\OneSignalApi.xml</DocumentationFile>
    <RepositoryUrl>https://github.com/OneSignal/onesignal-dotnet-api.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>C#, OneSignal, push</PackageTags>
    <Title>OneSignal API Library</Title>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageIcon>nuget_icon.png</PackageIcon>
    <PackageProjectUrl>https://github.com/OneSignal/onesignal-dotnet-api</PackageProjectUrl>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="JsonSubTypes" />
    <PackageReference Include="RestSharp.Serializers.NewtonsoftJson"/>
    <PackageReference Include="RestSharp"/>
    <PackageReference Include="Polly"/>
  </ItemGroup>
  <ItemGroup>
    <None Include="../../nuget_icon.png" Pack="true" PackagePath="" />
    <None Include="../../LICENSE" Pack="true" PackagePath="" />
  </ItemGroup>
</Project>
